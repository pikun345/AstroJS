    <div id={data.slug}  data-nextSlug={nextSlug} data-all={data}>
        <HyContentCard data={data}/>
     </div>
     <div class="flex flex-row justify-center items-center mb-40 xsm:col">
             
        <div class="flex flex-row pr-16 mr-32">
<!-- 1st div -->
        <div class="mt-5 ml-40 p-2 h-1/3 rounded-lg bg-white">
                
                <div class="pl-3 mr-3 mt-3">

                    <div class="">

