---
import '../../styles/zeta.css';
const {data,slug}=Astro.props;
import TagList from '../ZetaPages/TagList.astro'


const match="/"+slug+"/";


---
<div>
  <h1 class="text-3xl font-bold text-black ml-10 mb-7 mt-7"> Tags  </h1>
</div>

{data.map((tg) =>(
  tg.tags.nodes.map((data)=>(
  data.uri === match ? (
<div id="container">
    
   <div class="flex flex-col items-center mb-40 ml-11">
     <div id="divContainer" class="flex flex-wrap gap-2 mr-5">
        <TagList data={tg}/>
      </div>
     
  </div>
</div>

):null


  ))
  
 
))}
