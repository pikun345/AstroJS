---
import '../../styles/zeta.css';
const {data,slug}=Astro.props;
import Cat from './Cat.astro'


const match="/"+slug+"/";


---
<div>
  <h1 class="text-3xl font-bold text-black ml-10 mb-7 mt-7"> Category  </h1>
</div>
{data.map((cat) =>(
  cat.categories.nodes.map((data)=>(
  data.uri === match ? (
<div id="container">
    
   <div class="flex flex-col items-center mb-40 ml-11">
     <div id="divContainer" class="flex flex-wrap gap-2 mr-5">
        <Cat data={cat}/>
      </div>
     
  </div>
</div>

):null


  ))
  
 
))}
